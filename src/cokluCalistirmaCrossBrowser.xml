<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="sirali cross browser">
    <test name="cross browser chrome">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="tests.crossBrowserTest.C01_Dropdown"/>
            <class name="tests.crossBrowserTest.C02_BasicAuthetications"/>
        </classes>
    </test>
    <test name="cross browser edge">
        <parameter name="browser" value="edge"/>
        <classes>
            <class name="tests.crossBrowserTest.C03_Windowhandle"/>
        </classes>
    </test>
</suite>